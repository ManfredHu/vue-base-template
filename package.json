    "hu-tool": "^1.2.33",
    "jest": "^24.9.0",
    "js-cookie": "^2.2.1",
    "localforage": "^1.7.3",
    "vconsole": "^3.3.0",
    "vue": "^2.6.6",
    "vue-awesome-swiper": "^3.1.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
